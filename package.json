{
  "name": "yardtp-datetimepicker",
  "version": "2.0.3",
  "homepage": "https://nuclearhorsestudios.github.io/yardtp-datetimepicker/",
  "description": "Yet Another React Datetime Picker",
  "author": "Michael Lasky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NuclearHorseStudios/yardtp-datetimepicker.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "analyze": "source-map-explorer ./dist/index.js",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
